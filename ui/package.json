{
  "name": "ui",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:4000",
  "scripts": {
    "dev": "run-p dev:css dev:js",
    "dev:css": "chokidar 'src/css/tailwind.src.css' -c 'yarn build:css --env development'",
    "dev:js": "react-scripts start",
    "build": " yarn build:css && yarn build:js",
    "build:css": "postcss src/css/tailwind.src.css -o src/css/tailwind.css --env production",
    "build:js": "react-scripts build"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.0.1",
    "@auth0/auth0-spa-js": "^1.2.1",
    "apollo-boost": "^0.4.4",
    "autoprefixer": "^9.6.1",
    "graphql": "^14.5.3",
    "js-cookie": "^2.2.1",
    "postcss-cli": "^6.1.3",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.1.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^1.2.0",
    "chokidar-cli": "^2.0.0",
    "cssnano": "^4.1.10",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^1.1.2"
  }
}
