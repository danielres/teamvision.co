{
  "name": "e2e",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prepare": "run-p 'api store:test:replay' 'ui css:build'",
    "serve": "run-p 'api e2e' 'ui e2e'",
    "test": "run-s 'test:once --watch'",
    "test:once": "NODE_ENV=test env-cmd -f ../api/.env jest --runInBand server.test.js ",
    "api": "yarn workspace api run",
    "e2e": "yarn workspace e2e run",
    "ui": "yarn workspace ui run"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "babel-jest": "^25.5.1",
    "env-cmd": "^10.1.0",
    "jest": "^25.5.3",
    "jest-puppeteer": "^4.4.0",
    "npm-run-all": "^4.1.5",
    "puppeteer": "^3.0.2"
  }
}
